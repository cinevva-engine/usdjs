{
    "name": "@cinevva/usdjs",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "description": "Browser-first USD core (USDA/USDC) + composition + adapters. Engine-agnostic.",
    "license": "MIT",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "npm run build && node --test test/unit",
        "perf": "npm run build && node --test test/perf",
        "corpus:fetch": "node scripts/fetch-corpus.mjs",
        "corpus:curate": "npm run build && node scripts/curate-corpus.mjs",
        "corpus:curate:ftlab": "npm run build && node scripts/curate-ftlab.mjs",
        "test:corpus": "npm run build && node --test test/corpus",
        "test:all": "npm run test && npm run perf && npm run test:corpus"
    },
    "devDependencies": {
        "unzipper": "^0.10.14",
        "typescript": "^5.9.3"
    }
}