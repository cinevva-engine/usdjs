{
    "name": "@cinevva/usdjs",
    "version": "0.1.0",
    "type": "module",
    "description": "Pure TypeScript/JavaScript OpenUSD (USD) core: USDA/USDC/USDZ parsing, minimal composition, and utilities. Browser-first, engine-agnostic.",
    "license": "MIT",
    "author": "Cinevva",
    "homepage": "https://github.com/cinevva/usdjs#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cinevva/usdjs.git"
    },
    "bugs": {
        "url": "https://github.com/cinevva/usdjs/issues"
    },
    "keywords": [
        "usd",
        "openusd",
        "usda",
        "usdc",
        "usdz",
        "threejs",
        "parser",
        "composition",
        "typescript"
    ],
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src",
        "docs",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "npm run build && node --test test/unit",
        "perf": "npm run build && node --test test/perf",
        "corpus:fetch": "node scripts/fetch-corpus.mjs",
        "corpus:curate": "npm run build && node scripts/curate-corpus.mjs",
        "corpus:curate:ftlab": "npm run build && node scripts/curate-ftlab.mjs",
        "test:corpus": "npm run build && node --test test/corpus",
        "test:all": "npm run test && npm run perf && npm run test:corpus",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "unzipper": "^0.10.14",
        "typescript": "^5.9.3"
    },
    "publishConfig": {
        "access": "public"
    }
}